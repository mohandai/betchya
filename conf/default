
server {
	listen	80;
 
	root	/downloads/betchya;
 
	index	index.html index.htm index.nginx-debian.html;
 
	server_name	(54.252.136.227);
 
	location /static {
		root /downloads/betchya/betchyaApp;
	}
	
	location /media {
		alias	(/downloads/betchya/betchyaApp/static);
	}
 
	location / {
		include	uwsgi_params;
		uwsgi_pass	127.0.0.1:8000;
 
	}
 
}